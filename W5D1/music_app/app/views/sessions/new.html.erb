<section class="main-content">
  <h1>Log In</h1>

  <%= render 'shared/errors' %>

  <form action="<%= session_url %>" method="post">
    <%= auth_token %>

    <input id="email" type="text" name="session[email]" placeholder="Email">

    <br>

    <input id="password" type="password" name="session[password]" placeholder="Password">

    <br>

    <input type="submit">
  </form>
</section>
